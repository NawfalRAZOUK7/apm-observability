      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  $__timeGroupAlias(time, $__interval),\n  COUNT(*) AS requests\nFROM observability_apirequest\nWHERE $__timeFilter(time)\nGROUP BY 1\nORDER BY 1;",
          "refId": "A"
        }
      ],
      "title": "Requests per Interval",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "postgres",
        "uid": "timescaledb"
      },
      "fieldConfig": {
        "defaults": {
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  $__timeGroupAlias(time, $__interval),\n  100.0 * SUM(CASE WHEN status_code >= 500 THEN 1 ELSE 0 END) / NULLIF(COUNT(*), 0) AS error_rate\nFROM observability_apirequest\nWHERE $__timeFilter(time)\nGROUP BY 1\nORDER BY 1;",
          "refId": "A"
        }
      ],
      "title": "Error Rate (%)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "postgres",
        "uid": "timescaledb"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  $__timeGroupAlias(time, $__interval),\n  AVG(latency_ms) AS avg_latency_ms,\n  percentile_cont(0.95) WITHIN GROUP (ORDER BY latency_ms) AS p95_latency_ms\nFROM observability_apirequest\nWHERE $__timeFilter(time)\nGROUP BY 1\nORDER BY 1;",
          "refId": "A"
        }
      ],
      "title": "Latency (Avg / P95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "postgres",
        "uid": "timescaledb"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "id": 4,
      "options": {
        "showHeader": true
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT\n  endpoint,\n  COUNT(*) AS hits,\n  SUM(CASE WHEN status_code >= 500 THEN 1 ELSE 0 END) AS errors,\n  ROUND(AVG(latency_ms)::numeric, 2) AS avg_latency_ms\nFROM observability_apirequest\nWHERE $__timeFilter(time)\nGROUP BY endpoint\nORDER BY hits DESC\nLIMIT 10;",
          "refId": "A"
        }
      ],
      "title": "Top Endpoints (Hits)",
      "type": "table"
    },
    {
      "datasource": {
        "type": "postgres",
        "uid": "timescaledb"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "id": 5,
      "options": {
        "showHeader": true
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT\n  service,\n  COUNT(*) AS hits,\n  SUM(CASE WHEN status_code >= 500 THEN 1 ELSE 0 END) AS errors,\n  ROUND(AVG(latency_ms)::numeric, 2) AS avg_latency_ms\nFROM observability_apirequest\nWHERE $__timeFilter(time)\nGROUP BY service\nORDER BY hits DESC\nLIMIT 10;",
          "refId": "A"
        }
      ],
      "title": "Top Services (Hits)",
      "type": "table"
    }
